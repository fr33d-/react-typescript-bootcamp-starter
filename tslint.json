{
    "extends": ["tslint:recommended", "tslint-microsoft-contrib", "tslint-config-prettier"],
    "rules": {
        "completed-docs": false,
        "export-name": false,
        "import-name": false,
        "interface-name": false,
        "interface-over-type-literal": false,
        "match-default-export-name": false,
        "max-line-length": [true, 120],
        "newline-before-return": false,
        "no-angle-bracket-type-assertion": true,
        "no-any": [true, { "ignore-rest-args": true }],
        "no-backbone-get-set-outside-model": false,
        "no-implicit-dependencies": false,
        "no-import-side-effect": false,
        "no-object-literal-type-assertion": [true, { "allow-arguments": true }],
        "no-relative-imports": false,
        "no-submodule-imports": false,
        "no-suspicious-comment": false,
        "no-void-expression": false,
        "object-literal-sort-keys": false,
        "prefer-type-cast": false,
        "quotemark": [true, "double"],
        "semicolon": [true, "always", "ignore-bound-class-methods"],
        "strict-boolean-expressions": [true, "ignore-rhs", "allow-undefined-union"],
        "typedef": true,
        "variable-name": false
    }
}
